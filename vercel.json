{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.6" }
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "backend/app.py" },
      { "src": "/(.*)", "dest": "frontend/build/$1" }
    ]
  }
  